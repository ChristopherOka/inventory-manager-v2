{
    "rewrites": [
      {
        "source": "/:path*",
        "has": [
          {
            "type": "host",
            "value": "shop.martharave.com"
          }
        ],
        "destination": "/shop/:path*"
      }
    ],
    "redirects": [
        { "source": "/shop", "destination": "[https://shop.martharave.com](https://shop.martharave.com)" }
    ]
  }