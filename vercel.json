{
    "rewrites": [
        {
          "source": "/:path*",
          "has": [
            {
              "type": "host",
              "value": "shop.martharave.com"
            }
          ],
          "destination": "/shop/:path*"
        }
      ],
    "redirects": [
        { "source": "/store", "destination": "https://creature-world.vercel.app/" }
    ]
  }